
extend type Invoice implements Node {
    id: ID!
    customer: Customer
    invoiceDate: Date
    billingAddress: String
    billingCity: String
    billingState: String
    billingCountry: String
    billingPostalCode: String
    total: Float
    invoiceLines: [InvoiceLine]
}

extend type Query {
    invoice(id: ID!): Invoice
    invoices(first: Int, after: Int): [Invoice]
}



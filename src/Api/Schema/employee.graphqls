
extend type Employee implements Node {
    id: ID!
    lastName: String!
    firstName: String!
    title: String
    reportsTo: Employee
    birthDate: String
    hireDate: String
    address: String
    city: String
    state:  String
    country: String
    postalCode: String
    phone: String
    fax: String
    email: String
}

extend type Query {
    employee(id: ID!): Employee
    employees(first: Int, after: Int): [Employee]
}




extend type Customer implements Node {
    id: ID!
    lastName: String!
    firstName: String!
    company: String
    address: String
    city: String
    state:  String
    country: String
    postalCode: String
    phone: String
    fax: String
    email: String
    supportRep: Employee
}

extend type Query {
    customer(id: ID!): Customer
    customers(first: Int, after: Int): [Customer]
}

